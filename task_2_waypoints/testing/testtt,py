# Extract latitude and longitude
latitudes = [d[0] for d in data]
longitudes = [d[1] for d in data]

# Create the plot
plt.figure(figsize=(10, 8))
plt.plot(longitudes, latitudes, 'b-', linewidth=2, label='Path')  # Blue line for the path
plt.scatter(longitudes, latitudes, c='red', s=50, label='Waypoints')  # Red points for waypoints

# Labels and title
plt.xlabel('Longitude (degrees)')
plt.ylabel('Latitude (degrees)')
plt.title('GPS Path with Movement near Pune, India (36 Waypoints)')
plt.legend()

# Set equal aspect ratio to avoid distortion
plt.gca().set_aspect('equal')

# Zoom in on the path
min_lon, max_lon = min(longitudes), max(longitudes)
min_lat, max_lat = min(latitudes), max(latitudes)
margin = 0.0001  # Margin (~10 meters) to zoom in
plt.xlim(min_lon - margin, max_lon + margin)
plt.ylim(min_lat - margin, max_lat + margin)

# Add a grid for readability
plt.grid(True)

# Display the plot
plt.show()